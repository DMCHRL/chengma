<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <property name="now" value="now()" dbms="h2"/>

    <property name="now" value="now()" dbms="mysql"/>
    <property name="autoIncrement" value="true"/>

    <property name="floatType" value="float4" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle, mssql"/>

    <!--
        Added the entity MemberType.
    -->
    <changeSet id="20170712063829-1" author="jhipster">
        <createTable tableName="t_member_type">
            <column name="id" type="bigint(20)" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="c_member_code" type="varchar(25)">
                <constraints nullable="true" />
            </column>

            <column name="c_member_name" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="i_pay_fee" type="decimal(10,2)">
                <constraints nullable="true" />
            </column>

            <column name="i_create_by" type="bigint(20)">
                <constraints nullable="true" />
            </column>

            <column name="d_create_at" type="timestamp">
                <constraints nullable="true" />
            </column>

            <column name="i_last_update_by" type="bigint(20)">
                <constraints nullable="true" />
            </column>

            <column name="d_last_update_at" type="datetime">
                <constraints nullable="true" />
            </column>

            <column name="d_validaty" type="datetime">
                <constraints nullable="true" />
            </column>

            <column name="c_del_flag" type="varchar(1)">
                <constraints nullable="true" />
            </column>

            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>
        <dropDefaultValue tableName="t_member_type" columnName="d_create_at" columnDataType="datetime"/>
        <dropDefaultValue tableName="t_member_type" columnName="d_last_update_at" columnDataType="datetime"/>
        <dropDefaultValue tableName="t_member_type" columnName="d_validaty" columnDataType="datetime"/>

    </changeSet>
</databaseChangeLog>
